{
  "name": "@fabiospampinato/once",
  "description": "Wraps a function so that it's only ever executed once.",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "test": "fava",
    "test:watch": "fava -w",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/once/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/once.git"
  },
  "keywords": [
    "once"
  ],
  "devDependencies": {
    "fava": "^0.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "typescript-transform-export-interop": "^1.0.4"
  }
}
